    "@babel/core": "7.20.5",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-transform-async-to-generator": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@poziworld/custom-elements": "^1.3.2",
